@{
    ViewData["Title"] = "Home Page";
}
<div class="container">
    <div class="text-center">
        <h1 class="display-4">Aggregerings test data tool</h1>
    </div>
</div>
<div class="container pt-3">
    <hr/>
    <div class="row">
        <div class="col">
            <div class="form-group">
                <label for="exampleFormControlFile1">Upload your CSV file</label>
                <input type="file" class="form-control-file" id="exampleFormControlFile1">
            </div>
        </div>
    </div>
    <hr/>
    <div class="input-group mb-3">
        <div class="row">
            <div class="col">
                <select class="custom-select">
                    <option selected>Process type</option>
                    <option value="1">One</option>
                    <option value="2">Two</option>
                    <option value="3">Three</option>
                </select>
            </div>
            <div class="col">
                <div class="custom-control custom-checkbox">
                    <input type="checkbox" class="custom-control-input" id="customCheck1">
                    <label class="custom-control-label" for="customCheck1">Store basis data</label>
                </div>
            </div>
            <div class="col">
                <select class="custom-select">
                    <option selected>Job type</option>
                    <option value="1">One</option>
                    <option value="2">Two</option>
                    <option value="3">Three</option>
                </select>
            </div>

        </div>
        <div class="row">
            <div class="col">
                <div class="input-group mb-3">
                    <div class="input-group-prepend">
                        <span class="input-group-text" id="inputGroup-sizing-default">Job owner</span>
                    </div>
                    <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
                </div>
            </div>
            <div class="col">
                <div class="input-group mb-3">
                    <div class="input-group-prepend">
                        <span class="input-group-text" id="inputGroup-sizing-default">Grid area</span>
                    </div>
                    <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
                </div>
            </div>
            <div class="col">
                <select class="custom-select">
                    <option selected>Process variant</option>
                    <option value="1">One</option>
                    <option value="2">Two</option>
                    <option value="3">Three</option>
                </select>
            </div>
            <div class="col">
                <div class="input-group mb-3">
                    <div class="input-group-prepend">
                        <span class="input-group-text" id="inputGroup-sizing-default">Resolution</span>
                    </div>
                    <input type="text" class="form-control" aria-label="Sizing example input" aria-describedby="inputGroup-sizing-default">
                </div>
            </div>
        </div>

    </div>
    <div class="row">
        <div class="col">
            <button type="button" class="btn btn-primary btn-lg btn-block">Start job</button>
        </div>
    </div>
    <hr/>
    <div class="row pt-5">
        <h4>Results</h4>
        <table class="table">
            <thead>
            <tr>
                <th scope="col">Id</th>
                <th scope="col">State</th>
                <th scope="col">Created</th>
                <th scope="col">Owner</th>
                <th scope="col">ProcessType</th>
                <th scope="col">GridArea</th>
                <th scope="col">ProcessPeriodStart</th>
                <th scope="col">ProcessPeriodEnd</th>
                <th scope="col">JobType</th>
                <th scope="col">ExecutionEnd</th>
                <th scope="col">Files</th>
            </tr>
